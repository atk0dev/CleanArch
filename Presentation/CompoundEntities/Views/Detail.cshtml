@model CleanArchitecture.Application.CompoundEntities.Queries.GetCompoundEntityDetail.CompoundEntityDetailModel

@{
    ViewBag.Title = "CompoundEntity";
}

<h2>CompoundEntity</h2>
<table>
    <tr>
        <th>ID</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Date</th>
        <td>@Model.Date.ToString("d")</td>
    </tr>
    <tr>
        <th>SimpleEntity</th>
        <td>@Model.SimpleEntityName</td>
    </tr>
    <tr>
        <th>AnotherEntitiy</th>
        <td>@Model.AnotherEntitiyName</td>
    </tr>
    <tr>
        <th>A</th>
        <td>@Model.A</td>
    </tr>
    <tr>
        <th>B</th>
        <td>@Model.B</td>
    </tr>
    <tr>
        <th>AB</th>
        <td>@Model.AB</td>
    </tr>
</table>

